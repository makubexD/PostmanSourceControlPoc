        {
          "name": "TR - [Populate Inbound] Afid by Tfn",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "fbeca481-a18f-454c-a858-8f808e443cac",
                "exec": [
                  "var didTfn = pm.iterationData.get(\"Did\") || pm.collectionVariables.get(\"Did\") || pm.request.url.query.get(\"Tfn\");;\r",
                  "var campaignId = pm.iterationData.get(\"Campaign\") || pm.collectionVariables.get(\"Campaign\");\r",
                  "var afid = pm.iterationData.get(\"Afid\") || pm.collectionVariables.get(\"Afid\");\r",
                  "\r",
                  "var jsonData = pm.response.json();\r",
                  "\r",
                  "var testName = `DID/Tfn: ${didTfn}, CampaignId : ${campaignId.toUpperCase()}, expected AFID: ${afid} ---> AFID: ${jsonData.Afid}`;\r",
                  "\r",
                  "if(campaignId == \"MMS\") testName = `DID/Tfn: ${didTfn}, CampaignId : ${campaignId.toUpperCase()}, expected AFID: ${afid} ---> AFID: ${jsonData.AFID}`;\r",
                  "\r",
                  "pm.test(testName, function () {\r",
                  "    if (campaignId == \"MMS\"){ // Campaign with different Endpoint\r",
                  "        pm.expect(jsonData.AFID).to.eql(afid.toString());\r",
                  "    }else{\r",
                  "        pm.expect(jsonData.Afid).to.eql(afid.toString());\r",
                  "    }    \r",
                  "});\r",
                  ""
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "id": "411a262c-f06a-4541-82af-4f7ff8029e02",
                "exec": [
                  "var campaignId = pm.iterationData.get(\"CampaignId\") || pm.collectionVariables.get(\"CampaignId\");\r",
                  "\r",
                  "var campaign = \"cuy-\" + campaignId.toLowerCase();\r",
                  "if(campaignId == \"ZMA\") campaign = \"cuy-oma\";\r",
                  "\r",
                  "pm.collectionVariables.set(\"Campaign\", campaign);\r",
                  "pm.collectionVariables.set(\"AfidByTfnEndpoint\", \"afid-by-tfn\");"
                ],
                "type": "text/javascript"
              }
            }
